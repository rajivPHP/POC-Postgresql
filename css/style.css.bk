.bottom-ul {
	bottom: 0;
	margin-top: 20px !important;
}
.profile-pic {
	width: 60px;
	height: 60px;
}
.top-save-button {
	line-height: 64px;
}
.top-left-filter {
	margin-top: 25px;
	
}

.top-left-filter a {
	margin-right: 10px;
	
}

.top-left-filter .categry-filtr{background:#23a0d1; padding:8px; border-radius:24px; font-size:15px; color:#fff;}

.categry-filtr span img{width:28px; height:28px; margin-left:10px; margin-top:-3px;}

/*Des-2 */
.budget-img {
	width: 63%;
}
.timefrme {
	color: #999999;
	font-size: 28px;
	font-weight: 500;
}
.timefrme .media-pln {
	color: #353842;
	font-family: "Roboto", sans-serif;
	font-size: 30px;
	font-weight: 700;
	margin: 0 0 20px;
	padding: 0;
	display: inline-block;
}
.timefrme img {
	vertical-align: text-bottom;
}
.ur-schedule img {
	width: 100%;
}
.compai-otcome img {
