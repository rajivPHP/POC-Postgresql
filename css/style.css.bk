}
.explorer-detail-box {
	margin-top: 40px;
}
.modal-dialog { margin-top:150px;}
table tr td:first-child, table tr th:first-child { text-align:center;}


.inner-nav li { }

.inner-nav { }
.inner-nav-2 { position:absolute; left:100%; z-index:999; width:175px; background:#29a2d2; top:0; bottom:0; height:100%;}
.inner-nav-2 li a {text-align:left; padding-left:10px !important; display:inline-block; border:none; border-bottom:1px solid #207ca0;}

.inner-nav-2 li a span {padding-right:10px;}
.inner-nav-2 li a span img {width:22px; height:22px;}
.inner-nav-2 .inner-close{text-align:left; padding-left:10px; padding-right:10px;}
.inner-close {padding:10px 0px; color:#fff; border-bottom:1px solid #207ca0; font-size:15px;}
.inner-close span img{width:12px; height:12px;}



.brand-affinnity { position:absolute; left:100%; z-index:99; width:175px; background:#29a2d2; top:145px;top:0; bottom:0;}
.brand-affinnity li{}
.brand-affinnity li a {text-align:left; padding-left:10px !important; display:inline-block; border:none;}

.brand-affinnity li a span {padding-right:10px;}
.brand-affinnity li a span img {width:22px; height:22px;}
.brand-affinnity .inner-close{text-align:left; padding-left:10px; padding-right:10px;}
.inner-close {padding:10px 0px; color:#fff; border-bottom:1px solid #207ca0; font-size:15px;}
.inner-close span img{width:12px; height:12px;}
.sidebar > ul > li > ul > li {border-bottom:1px solid #207ca0; }



/* suresh */


/* make sidebar nav vertical */ 
@media (min-width: 768px) {
  .sidebar-nav .navbar .navbar-collapse {
    padding: 0;
    max-height: none;
  }
  .sidebar-nav .navbar ul {
    float: none;
  }
  .sidebar-nav .navbar ul:not {
    display: block;
  }
  .sidebar-nav .navbar li {
    float: none;
    display: block;
  }
  .sidebar-nav .navbar li a {
    padding-top: 12px;
    padding-bottom: 12px;
  }
}
